public class Boletin7_3 {

    public static void main(String[] args) {

        int num;

        String auxNum = JOptionPane.showInputDialog("Introduzca un nÃºmero: ");
        num = Integer.parseInt(auxNum);
        if (num >= 0) {

            JOptionPane.showMessageDialog(null, "+");

        } else {

            JOptionPane.showMessageDialog(null, "-");

        }

    }

}
